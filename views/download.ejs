<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/header') %> 

    <style>
        
       /*The nav bar styles*/
       nav{
           position: relative;
           width: 100%;
           padding: 4px 24px;   
           background-color: #340F27;
       }

       nav a img{
           width: 60px;
           height: 60px;
       }
       nav a{
         list-style: none;
         text-decoration: none;

       }
       nav a span{
         display: inline;
         color: #D91570;
         font-size: 20px;   
         text-align: center;
         margin-left: 40%;
       }
       /* div h3{
            margin: 0 auto;
       } */
    </style>
</head>
<body>
    <nav>
       <a href="<%= home %> " class="logo"><img src="kist.svg" alt="kist logo"><span><%= screen %></span></a>
    </nav>
    <%# include('./partials/nav') %> 
     <!--This code contains the tags for the generated timetable-->
     <section class="table-container-final">
        <div>
            <h3><%= status %> </h3>
        </div> 
        <div class="table-container-wrap-final">
            <div class="table-overflow">
                <table>
                  <thead>
                      <th>Day</th>
                      <th>Task</th>
                      <th>Start Time</th>
                      <th>End Time</th>
                  </thead>
                  <tbody>
                    <% schedules.forEach( schedule => {  %> 
                      <tr>
                          <td class="left-nav"><%= schedule.day %></td>
                          <td><%= schedule.title %></td>
                          <td><%= schedule.startTime %></td>
                          <td><%= schedule.endTime %></td>
                      </tr>
                  </tbody>
                  <% }) %>
              </table>
            </div>
            <div class="action-print">
                <button class="download"><a href="<%= file  %>" download> Download </a></button>
            </div>
        </div>
    </section>

</body>
</html>
